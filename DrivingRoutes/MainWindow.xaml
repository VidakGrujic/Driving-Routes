<Window x:Class="DrivingRoutes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrivingRoutes"
        mc:Ignorable="d"
        xmlns:gmf="clr-namespace:GMap.NET.WindowsForms;assembly=GMap.NET.WindowsForms"
        xmlns:gm="clr-namespace:GMap.NET;assembly=GMap.NET.Core"
        xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        Title="MainWindow" Height="600" Width="900">
    <Grid>
        <DockPanel>
            <StackPanel Height="50" DockPanel.Dock="Top">
                <Button x:Name="LoadModelButton" Width="200" HorizontalAlignment="Center" Margin="0,15,0,0" Click="LoadModelButton_Click">Load Model</Button>
            </StackPanel>
            <WindowsFormsHost>
                <gmf:GMapControl x:Name="gmap" CanDragMap="True" MarkersEnabled="True" RoutesEnabled="True" MinZoom="2" MaxZoom="18" Zoom="14"/>
            </WindowsFormsHost>
        </DockPanel>
    </Grid>
</Window>
